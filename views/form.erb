<h1>
  Max's Living Celebrity Age Checker
</h1>
<form action="/test">
  <div class="flex">
  <div>
    <label for="article_title">
      Enter the living famous person whose age you'd like to look up:
    </label>
    <input id="title" name="article_title" type="text" required>
  </div>
  <div>
    <label for="guess_age">
      Enter how old you think they are:
    </label>
    <input id="guess" name="guess_age" type="number" placeholder="numbers only, no letters" required>
  </div>
  <div>
  <button>
    Go
  </button>
  </div>
  </div>
</form>
